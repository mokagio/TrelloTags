{
  "manifest_version": 2,
  "name": "TrelloTags",
  "version": "0.1",
  "description": "Use tags in your Trello cards, because sometimes 6 labels are not enough.",
  "content_scripts": [
    {
      "matches": [ 
        "https://trello.com/*" 
      ],
      "js": [ 
        "jquery-2.0.3.min.js", 
        "trellotags.js" 
      ],
      "css": [ 
        "trellotags.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "jquery-2.0.3.min.js"
  ]
}